<table class="table table-condensed">
{% for columns in records %}
    {% if loop.first %}
    <thead>
        {% include 'WeavingTheWebDashboardBundle:Perspective/Table/Head:row.html.twig' %}
    </thead>
    {% endif %}
    {% if loop.index == 1 %}
    <tbody{% if enabled_search is defined %} id="jetsContent"{% endif %}>
    {% endif %}
        {% include 'WeavingTheWebDashboardBundle:Perspective/Table/Body:row.html.twig' %}
    {% if loop.last %}
    </tbody>
    {% endif %}
{% endfor %}
</table>
