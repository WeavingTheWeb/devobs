{
    "name": "weaving-the-web/experiments",
    "description": "Sharing experiments for fun's sake",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:thierrymarianne/FOSTwitterBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:thierrymarianne/twitteroauth.git"
        }
    ],
    "require": {
        "braincrafted/bootstrap-bundle": "2.0.1",
        "doctrine/common": "2.5.1",
        "doctrine/dbal": "2.5.2",
        "doctrine/inflector": "1.*",
        "doctrine/orm": "2.5.2",
        "doctrine/doctrine-bundle": "1.5.2",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "elao/web-profiler-extra-bundle": "dev-master",
        "ext-openssl": "^0.0.0",
        "ext-zip": "^1.11",
        "facebook/php-sdk-v4": "4.0.*",
        "fabpot/goutte": "3.1.1",
        "friendsofsymfony/elastica-bundle": "~3.1",
        "friendsofsymfony/jsrouting-bundle": "1.5.*",
        "friendsofsymfony/oauth-server-bundle": "1.4.2",
        "friendsofsymfony/rest-bundle": "0.*",
        "friendsofsymfony/twitter-bundle": "dev-master#f837424a069425721c1a744bdc60bb2869a8c691",
        "friendsofsymfony/user-bundle": "dev-master",
        "guzzlehttp/guzzle": "6.1.0",
        "incenteev/composer-parameter-handler": "2.1.*",
        "jms/di-extra-bundle": "1.6.*",
        "jms/security-extra-bundle": "1.5.*",
        "jms/serializer-bundle": "dev-master",
        "kertz/twitteroauth": "dev-master",
        "knplabs/knp-markdown-bundle": "1.3.*",
        "kriswallsmith/assetic": "1.3.1",
        "liip/url-auto-converter-bundle": "1.0.1",
        "oldsound/rabbitmq-bundle": "dev-master",
        "oneup/uploader-bundle": "1.3.1",
        "php": ">=5.4.8",
        "phpseclib/phpseclib": "2.0",
        "predis/predis": "1.0.3",
        "ramsey/uuid": "3.0.1",
        "snc/redis-bundle": "1.1.9",
        "symfony/assetic-bundle": "2.7.0",
        "symfony/monolog-bundle": "2.6.*",
        "symfony/symfony": "2.7.5",
        "sensio/distribution-bundle": "3.*",
        "sensio/framework-extra-bundle": "3.*",
        "sensio/generator-bundle": "2.3.*",
        "smart-core/accelerator-cache-bundle": "1.1.0",
        "sp/bower-bundle": "dev-master#9fd50491bbfd244042fdb6e81f80ca2223b393ac",
        "themattharris/tmhOAuth": "dev-master",
        "twig/extensions": "1.0.*"
    },
    "require-dev": {
        "behat/behat": "3.0.15",
        "behat/mink": "1.6.0",
        "behat/mink-selenium2-driver": "1.2.0",
        "behat/mink-browserkit-driver": "1.2.0",
        "behat/symfony2-extension": "2.1.0",
        "behat/mink-extension": "2.1.0",
        "behat/mink-goutte-driver": "1.2.0",
        "jarnaiz/behat-junit-formatter": "1.3.1",
        "phpspec/phpspec": "2.0.*@dev",
        "phpunit/phpunit": "4.3.5",
        "symfony/phpunit-bridge": "2.7.6",
        "weaving-the-web/framework-extra-bundle": "dev-master#93d448d97ab8f9946acb489257424f2f572b9c49"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "symfony-vendor-dir": "vendor"
    }
}
